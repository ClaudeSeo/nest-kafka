{
  "name": "@claudeseo/nest-kafka",
  "version": "0.0.1",
  "description": "simple integrate kafka.js with nest.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "branches": [
      "main",
      "next"
    ]
  },
  "scripts": {
    "lint": "gts lint",
    "lint:fix": "gtx fix",
    "clean": "gts clean",
    "compile": "tsc",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClaudeSeo/nest-kafka.git"
  },
  "keywords": [
    "nestjs",
    "kafkajs"
  ],
  "author": "ClaudeSeo <ehdaudtj@gmail.com>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/ClaudeSeo/nest-kafka/issues"
  },
  "homepage": "https://github.com/ClaudeSeo/nest-kafka#readme",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/microservices": "^8.2.6",
    "kafkajs": "^1.15.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@nestjs/config": "^1.1.6",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.18.10",
    "gts": "^3.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
